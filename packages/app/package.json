  "private": true,
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/connect-redis": "^0.0.15",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "4.17.0",
    "@types/express-session": "^1.17.2",
    "@types/express-ws": "^3.0.0",
    "@types/ioredis": "^4.17.8",
    "@types/redis": "^2.8.28",
    "@types/stats.js": "^0.17.0",
    "@types/uuid": "^8.3.0",
    "builtin-modules": "^3.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.16.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy-assets": "^2.0.1",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-generate-html-template": "^1.6.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "dev:client-build": "rollup -c -w",
    "dev:server-build": "tsc -b tsconfig.server.json -w",
    "dev:server": "nodemon ./dist/server.js",
    "debug:server": "node --inspect ./dist/server.js",
    "dev": "run-p -cln dev:server-build dev:client-build",
    "test": "echo 'skipping'",
    "watch": "yarn dev",
    "start": "yarn build && node ./dist/server.js",
    "build": "yarn build:server && yarn build:client",
    "build:server": "tsc -b tsconfig.server.json",
    "build:client": "rollup -c",
    "prebuild": "yarn clean",
    "clean": "rimraf {dist,tsconfig.server.tsbuildinfo}",
    "generate-dep-graph": "madge --image ../../dependency-graphs/app.svg ./src"
  },
  "dependencies": {
    "@dungeon-crawler/core": "^0.1.0",
    "@dungeon-crawler/network": "^0.1.0",
    "@dungeon-crawler/renderer": "^0.1.0",
    "@dungeon-crawler/runtime": "^0.1.0",
    "@dungeon-crawler/world-configs": "^0.1.0",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.2",
    "builtin-modules": "^3.1.0",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-ws": "^4.0.0",
    "ioredis": "^4.19.2",
    "stats.js": "^0.17.0",
    "utf-8-validate": "^5.0.3",
    "uuid": "^8.3.2"
  }
}
